<!-- Know Your Problems -->
<section id="knowurprob" class="project-container container">
    <div class="division"></div>
    <div class="content-text">
        <h2>Know About Your Problem</h2>
        <p>Explore the nature of your concerns, learn effective self-help strategies, and discover professional treatments available.</p>
    </div>
    <form action="/submit" method="post" onsubmit="openTextInNewTab(event)">
        <div class="input-wrapper">
            <input class="input-box" type="text" id="question" name="problem" placeholder="Anxiety">
